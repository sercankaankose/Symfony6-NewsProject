{% extends  'base.html.twig' %}


{% block title %}NEWS{% endblock %}

{% block body %}
    <style>
        .thumbnail img {
            max-height: 400px;
            width: auto;
        }

    </style>
    <main id="main">

        <!-- ======= Hero Slider Section ======= -->
        <div id="carouselExampleCaptions" class="carousel slide">
            {% for message in app.flashes('register') %}
                <div class="alert alert-danger">
                    {{ message }}
                </div>
            {% endfor %}
            <div class="col-12">
                <div class="carousel-indicators">

                    <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="0" class="active"
                            aria-current="true" aria-label="Slide 1"></button>
                    <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="1"
                            aria-label="Slide 2"></button>
                    <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="2"
                            aria-label="Slide 3"></button>
                </div>
                <div class="carousel-inner">
                    <div class="carousel-item active">
                        {% set new = popularNews|slice(0, 1)|first %}
                        {% if new is not empty %}

                        <a href="{{ path('app_post',{'id': new.id}) }}" class="d-block w-100">
                            <img src="{{ asset(new.image) }}" class="d-block w-100" alt="1"
                                 style="object-fit: cover; max-height: 80vh; max-width: 100%;"/>
                            <div class="carousel-caption d-none d-md-block text-bg-dark">
                                <h2>{{ new.title }}</h2>
                                <p>{{ new.content|slice(0, 400) }}</p>
                            </div>
                        </a>
                    </div>
                    {% else %}
                    {% endif %}
                    <div class="carousel-item">
                        {% set new = popularNews|slice(1, 2)|first %}
                        {% if new is not empty %}

                        <a href="{{ path('app_post',{'id': new.id}) }}" class="d-block w-100">
                            <img src="{{ asset(new.image) }}" class="d-block w-100" alt="2"
                                 style="object-fit: cover; max-height: 80vh; max-width: 100%;"/>
                            <div class="carousel-caption d-none d-md-block text-bg-dark">
                                <h2>{{ new.title }}</h2>
                                <p>{{ new.content|slice(0, 400) }}</p>
                            </div>
                        </a>
                    </div>
                    {% else %}
                    {% endif %}
                    <div class="carousel-item">
                        {% set new = popularNews|slice(2, 3)|first %}
                        {% if new is not empty %}

                        <a href="{{ path('app_post',{'id': new.id}) }}" class="d-block w-100">
                            <img src="{{ asset(new.image) }}" class="d-block w-100" alt="3"
                                 style="object-fit: cover; max-height: 80vh; max-width: 100%;"/>
                            <div class="carousel-caption d-none d-md-block text-bg-dark">
                                <h2>{{ new.title }}</h2>
                                <p>{{ new.content|slice(0, 400) }}</p>
                            </div>
                        </a>
                    </div>
                </div>

                {% else %}
