                                <div class="post-meta"><span class="date">{{ new.category.categoryname }}</span> <span
                                            class="mx-1">&bullet;</span>
                                    <span>{{ new.publishedAt|date('M jS Y') }}</span></div>
                                <h2><a href={{ path('app_post',{'id':new.id}) }}>{{ new.title }}</a>
                                </h2>
                                <p class="mb-4 d-block">{{ new.content|slice(0, 400) }}...</p>

