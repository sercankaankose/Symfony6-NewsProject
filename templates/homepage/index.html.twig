                                <div class="post-entry-1 border-bottom">
                                    <div class="post-meta"><span
                                                class="date">{{ new.category.categoryname }}</span> <span
                                                class="mx-1">&bullet;</span>
                                        <span>{{ new.publishedAt|date('M jS Y') }}</span></div>
                                    <a href="{{ path('app_public_profile',{'id':new.author.id}) }}">
                                        <span class="author mb-3 d-block">{{ new.author.name }} {{ new.author.surname }}</span>
                                    </a>
                                    <h2 class="mb-2"><a href="{{ path('app_post',{'id':new.id}) }}">
                                            {{ new.title }}
                                        </a></h2>
                                </div>
                            {% else %}
                            {% endif %}
                        {% endfor %}
                    </div>


                </div>
            </div>
        </section>
        <!-- End Finance Category Section -->

    </main>
    <!-- End #main -->

